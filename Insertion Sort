public class Insertion_Sort
{
    public static void main(String[] args)
    {
        int arr[]={5,10,2,6,8,5};
        for (int i = 1; i <arr.length; i++)
        {
            int key=arr[i];
            int j=i-1;
        while ( j>=0 && arr[j]>key)                 // for shifting the elements
        {
            arr[j+1]=arr[j];
            j--;
        }
        arr[j+1]=key;
        }
        for (int i:arr) {System.out.println(i);}
    }
}
